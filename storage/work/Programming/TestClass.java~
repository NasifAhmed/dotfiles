public class TestClass {
  public static void main(String[] args) {
    int[] a = {100, 2, 40, 6, 1};
    int[] b = {1, 2, 213, 111, 1111};
    printAll(b);
    
    largestNumber(a);
    smallestNumber(a);
  }
  
  public static void printkoro(String input) {
    System.out.println(input);
  }
  
  public static int adder(int a, int b) {
    return a+b;
  }
  
  public static void printAll(int[] array) {
    for(int i=0 ; i<array.length ; i++) {
      printkoro("Element : "+array[i]);
    }
  }
  
  public static int calculator(int a, int b, char sign) {
    if(sign == '+') {
      return adder(a,b);
    } else if(sign == '-') {
      return a-b;
    } else if(sign == '*') {
      return a*b;
    } else if(sign == '/') {
      return a/b;
    } else {
      return 0;
    }
  }
  
  
}