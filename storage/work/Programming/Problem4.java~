public class Problem4 {
  public static void main(String[] args) {
    int limit = 10;
    int count = 1;
    while() {
      
      if(count>=limit) {
        break;
      }
      
      for(int i=1 ; i<=count ; i++) {
        System.out.print('*');
      }
      System.out.print("/n");
      count++;
    }
  }
}