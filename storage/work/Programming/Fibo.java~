public class Fibo {
  public static void main(String[] args) {
    int input = 3;
    FizzBuzz(15);
    
  }
  
  public static void FizzBuzz(int n) {
    String output = "";
    if(n%3!=0 && n%5!=0){
      output = "i";
    } else {
      if(n%3==0) {
        output = output + "Fizz";
      }
      if(n%5==0) {
        output = output + "Buzz";
      }
    }
    System.out.println(output);
  }
}