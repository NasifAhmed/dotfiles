public class Problems {
  public static void main(String[] args) {
    int limit = 100;
    for(int j=1 ; j<=limit ; j++) {
      for(int i=1 ; i<=j ; i++) {
        System.out.print("*");
      }
      for(int i=1 ; i<=limit-j ; i++) {
        System.out.print(".");
      }
      System.out.println();
    
    }
  }
}